{
  "name": "register-package-name",
  "version": "0.2.0",
  "description": "Register blank npm package with given name.",
  "keywords": [
    "npm",
    "register",
    "package",
    "name",
    "publish",
    "register-package",
    "blank",
    "empty"
  ],
  "author": "Yurii Brusentsov <yuriybrusentsov@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc && swc ./dist/index.js -o ./dist/index.js",
    "bin": "ts-node ./src/index.ts",
    "format": "prettier --write ."
  },
  "bin": "dist/index.js",
  "dependencies": {
    "commander": "^11.1.0",
    "core-js": "3.17"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.6",
    "@swc/core": "^1.3.107",
    "@types/node": "^20.11.15",
    "prettier": "3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "homepage": "https://github.com/goveo/register-package-name#readme"
}
